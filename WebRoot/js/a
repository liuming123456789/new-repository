        "broad" == r ? (t += "<label>" + o.phone + "</label>", t += judgeEmpty(o.customName) ? "<span>" + o.PROVINCE_NAME + "&nbsp;" + o.CITY_NAME + "</span>" : "<span>" + o.customName + "</span>", 
        t += "</li>") : (t += "<label>" + o.phone + "</label>", t += judgeEmpty(o.customName) ? "<span>&nbsp;" + o.CITY_NAME + "</span>" : "<span>" + o.customName + "</span>", 
        t += "</li>");
    }), judgeEmpty(t) || (t += "<li onclick=\"clearHistoryNum('" + r + '\');"><i class="cOrange">清空历史号码</i> </li>'), 
    t;
