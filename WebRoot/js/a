function checkBroadBandNumber(e, n) {
    if ("与宽带绑定的固话" == n) {
        if (!/^(010|02[0-9]|0[3-9][0-9]{2})\-?([2-9][0-9]{6,7})$/.test(e)) return "fixError";
    } else if ("宽带拨号账号" == n) {
        if (!/^([\w-\.\@]){1,30}$/.test(e)) return "accountError";
    } else if ("统一宽带编码" == n && !/^(010|02[0-9]|0[3-9][0-9]{2})\-?([0][0-9]{6,7})$/.test(e)) return "codeError";
    return "ok";
}

