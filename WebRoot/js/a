}, isBroadbandProvinceOnline = function() {
    var e = "broadbandPackYear/isBroadBandProvinceOnline";
    broadband.isOnline = "", $.ajax({
        type: "get",
        data: {
            numberprovince: broadband.broadProvCode
        },
        dataType: "jsonp",
        jsonp: "callback",
        jsonpCallback: "broadbandRule",
        url: serviceBankchargeURL + (ISWAP ? e : e)
    }).success(function(e) {
        if (broadband.isOnline = e.isOnline, broadband.isHideCurPduc = e.isHideCurProduct, 
        !judgeEmpty(broadband.isOnline) || broadband.isOnline) {
            if (!validateParameters()) return;
            broadband.submitFlag && broadbandYearCheck();
        } else if (judgeEmpty(broadband.isOnline) || !broadband.isOnline) return $(".userInfo").hide(), 
        $(".User_information").hide(), $(".broadbtn button").addClass("gray"), $(".chosen").hide(), 
        $(".hasChosen").hide(), $(".unQuery").show(), $(".close").show(), $("#errormsg").html("该省份宽带包年没有上线!"), 
        void errormsgshow();
        computedHei();
    }).error(function(e) {
        return "busy" == e ? ($("#errormsg").html("系统繁忙,请稍后稍后再试!"), void errormsgshow()) : void 0;
    });
}, queryProduction = function() {
    if ($(".loadingdiv").show(), !validateQuerybdProdcut()) return void $(".loadingdiv").hide();
    var uri = "broadbandPackYear/productQuery", phoneNo = "";
    judgeEmpty(broadband.netCode) || judgeEmpty(broadband.areaCode) || (phoneNo = broadband.bdNumber), 
    $.ajax({
        type: "post",
        data: {
            numberType: broadband.netCode,
            "commonBean.channelType": CHANNEL_TYPE,
            "commonBean.areaCode": broadband.areaCode,
            "commonBean.phoneNo": phoneNo,
            "commonBean.netAccount": broadband.bdNumber,
            "commonBean.provinceCode": broadband.broadProvCode,
            "commonBean.cityCode": broadband.broadCityCode
        },
        dataType: "json",
        url: serviceBankchargeURL + (ISWAP ? uri : uri)
    }).success(function(data) {
        $(".loadingdiv").hide();
        var list = "";
        if (judgeEmpty(data) || "success" != data.out) return "busy" == data.out ? ($(".loadingdiv").hide(), 
        broadband.secstate = data.secstate, $("#errormsg").html("您的操作太频繁，请稍后再试！"), void errormsgshow()) : ($(".loadingdiv").hide(), 
        broadband.secstate = data.secstate, $("#errormsg").html(data.out), void errormsgshow());
        isBroadBalanceOnline(broadband.broadProvCode, broadband.bdNumber), isHidenPrudct(broadband.broadProvCode, broadband.bdNumber);
        var productInfo = eval("(" + data.showList + ")");
        broadband.secstate = data.secstate, broadband.queryBroadCode = data.queryBoradCode, 
        ismonthquery = "1" == data.BroadbandPackYearBean.productType, $.each(productInfo, function(e, d) {
            0 == e ? (broadband.bdProductId = d.productId, broadband.discntCode = d.discntCode, 
            broadband.selNoChange = !1, broadband.brandNumber = d.brandNumber, broadband.selfspeed = d.brandSpeed, 
            broadband.selmktEndDate = d.mktEndDate, broadband.defaultFee = d.discntFee / 100, 
            broadband.defaultSpeed = d.brandSpeed, broadband.defaultType = d.accessType, broadband.defaultEnable = d.startEnable, 
            broadband.defaultDiscntName = d.discntName, broadband.broadJudgeChargeStr = d.productId + d.discntCode, 
            (ismonthquery ? monthproductHidden : broadband.isHideCurPduc) || (broadband.selProductID = d.productId, 
            broadband.selDiscntCode = d.discntCode, broadband.broadSelJudgeChargeStr = d.productId + d.discntCode, 
            queryUserInfo(!1)), (ismonthquery ? monthproductHidden : broadband.isHideCurPduc) || (list += '<li class="current"><a onclick="refer(\'' + d.productId + "','" + d.discntFee / 100 + "','" + d.brandSpeed + "','" + d.accessType + "','" + d.startEnable + "','" + d.discntName + "','" + d.discntCode + "','" + d.brandNumber + "','" + d.productId + "'," + broadband.selNoChange + ')"><span  id="' + d.productId + d.discntCode + '"><i class="sign" style="display:none"></i><i class="continueIcon" ></i><i class="unchecked"></i><i class="checked" style="display:none"></i><div class="ownProduct"><span class="pName">宽带包年</span><span class="currentAmount"></span><span class="effectiveTime">' + d.mktEndDate.replace(/-/gm, "/") + "</span></div></span></a></li>")) : ismonthquery && monthproductHidden && 1 == e ? (broadband.selNoChange = !0, 
            refer(d.productId, d.discntFee / 100, d.brandSpeed, d.accessType, d.startEnable, d.discntName, d.discntCode, d.brandNumber, d.productId, broadband.selNoChange), 
            list += '<li class="prod" id="' + d.productId + d.discntCode + '"><a onclick="refer(\'' + d.productId + "','" + d.discntFee / 100 + "','" + d.brandSpeed + "','" + d.accessType + "','" + d.startEnable + "','" + d.discntName + "','" + d.discntCode + "','" + d.brandNumber + "','" + d.productId + "'," + broadband.selNoChange + ')"><i class="unchecked"></i><i class="checked" style="display:none"></i><span class="first"><label class="size">' + d.brandSpeed + '</label><em>宽带</em></span><span class="sec"><label class="bdPrice">' + d.discntFee / 100 + "元</label><em>/" + d.brandNumber + "个月</em></span></a></li>") : productHidden || (broadband.IsCanChange = !0, 
            1 == e && broadband.isHideCurPduc && (broadband.selNoChange = !0, refer(d.productId, d.discntFee / 100, d.brandSpeed, d.accessType, d.startEnable, d.discntName, d.discntCode, d.brandNumber, d.productId, broadband.selNoChange)), 
            broadband.accessType = d.accessType, list += '<li class="prod" id="' + d.productId + d.discntCode + '"><a onclick="refer(\'' + d.productId + "','" + d.discntFee / 100 + "','" + d.brandSpeed + "','" + d.accessType + "','" + d.startEnable + "','" + d.discntName + "','" + d.discntCode + "','" + d.brandNumber + "','" + broadband.selProductID + "'," + broadband.IsCanChange + ')"><i class="unchecked"></i><i class="checked" style="display:none"></i><span class="first"><label class="size">' + d.brandSpeed + '</label><em>宽带</em></span><span class="sec"><label class="bdPrice">' + d.discntFee / 100 + "元</label><em>/" + d.brandNumber + "个月</em></span></a></li>");
        }), $(".proBackground").html(list);
    }).error(function() {
        $(".loadingdiv").hide(), $("#errormsg").html("请求失败，请稍后再试！"), errormsgshow();
    });
}, Chosen = function() {
    judgeEmpty(broadband.bdProductId) || judgeEmpty(broadband.selProductID) ? $(".current .checked").show() : judgeNoEmptyAndEqls(broadband.broadJudgeChargeStr, broadband.broadSelJudgeChargeStr) ? ($(".sign").hide(), 
    $(".continueIcon").show(), $("#" + broadband.broadJudgeChargeStr + " .checked").show()) : ($(".sign").show(), 
    $(".checked").hide(), $(".unchecked").show(), $(".continueIcon").hide(), $("#" + broadband.broadSelJudgeChargeStr + " .checked").show());
}, refer = function(e, d, a, r, o, n, t, i, b, s) {
    broadband.selProductID = e, broadband.broadSelJudgeChargeStr = e + t, broadband.bdProductPrice = d, 
    broadband.brandSpeed = a, broadband.startEnable = o, broadband.bdDiscntName = n, 
    broadband.accessType = r, broadband.selDiscntCode = t, broadband.IsChange = s, broadband.brandNumber = i, 
    $(".unchecked").show(), $(".checked").hide(), $(".productionPage").hide(), $(".mainPage").show(), 
    $(".unQuery").hide(), $(".chosen").hide(), $(".close").hide(), $("#messageTipsDiv").show(), 
    recognizeIsChange(broadband) ? ($(".hasChosen").show(), $(".continueIcon").show(), 
    "0" == o ? ($(".hasStartTips").html("立即生效"), $(".hasproductInfoDetail").html(broadband.brandSpeed + "宽带 &nbsp;" + broadband.bdProductPrice + "元/" + broadband.brandNumber + "个月"), 
    $(".price").html(Number(broadband.bdProductPrice).toFixed(2) + "元"), $(".payFeeMoney b").html(Number(broadband.bdProductPrice).toFixed(2)), 
    $(".total b").html(Number(broadband.bdProductPrice).toFixed(2))) : "1" == o ? ($(".hasStartTips").html("次月生效"), 
    $(".hasproductInfoDetail").html(broadband.brandSpeed + "宽带 &nbsp;" + broadband.bdProductPrice + "元/" + broadband.brandNumber + "个月"), 
    $(".price").html(Number(broadband.bdProductPrice).toFixed(2) + "元"), $(".payFeeMoney b").html(Number(broadband.bdProductPrice).toFixed(2)), 
    $(".total b").html(Number(broadband.bdProductPrice).toFixed(2))) : "2" == o && ($(".hasStartTips").html("到期生效"), 
    $(".hasproductInfoDetail").html(broadband.brandSpeed + "宽带 &nbsp;" + broadband.bdProductPrice + "元/" + broadband.brandNumber + "个月"), 
    $(".price").html(Number(broadband.bdProductPrice).toFixed(2) + "元"), $(".payFeeMoney b").html(Number(broadband.bdProductPrice).toFixed(2)), 
    $(".total b").html(Number(broadband.bdProductPrice).toFixed(2))), queryUserInfo(!0)) : ($(".hasChosen").hide(), 
    $(".sign").show(), $(".chosen").show(), $(".proEndTime").html(broadband.selmktEndDate.replace(/-/gm, "/")), 
    queryUserInfo(!1));
}, queryUserInfo = function(e) {
    $(".submit b").html(""), $(".loadingdiv").show(), $.ajax({
        url: serviceBankchargeURL + "broadbandPackYear/checkPayAmount",
        data: {
            "secstate.state": broadband.secstate,
            selProductID: broadband.selProductID,
            selDiscntCode: broadband.selDiscntCode,
            selIsChange: e
        },
        dataType: "json",
        type: "post",
        success: function(e) {
            if ($(".loadingdiv").hide(), "success" != e.out) return $("#errormsg").html(e.out), 
            void errormsgshow();
            if (showUserInfo(e), broadband.bdProductPrice = e.DiscntInfo.payAmount / 100, judgeNoEmptyAndEqls(broadband.broadJudgeChargeStr, broadband.broadSelJudgeChargeStr) && ($(".unQuery").hide(), 
            $(".hasChosen").hide(), $(".sign").show(), $(".chosen").show(), $(".proNameWord").show(), 
            $(".User_information").show(), $(".broadbtn button").removeClass("gray"), $(".proEndTime").html(broadband.selmktEndDate.replace(/-/gm, "/")), 
            $(".currentAmount").html(broadband.bdProductPrice + "元").append('<span class="year">/' + broadband.brandNumber + "个月</span>")), 
            $(".User_information").show(), $(".broadbtn button").removeClass("gray"), $(".proEndTime").html(broadband.selmktEndDate.replace(/-/gm, "/")), 
            $(".currentAmount").html(broadband.bdProductPrice + "元").append('<span class="year">/' + broadband.brandNumber + "个月</span>"), 
            $(".submit b").html(broadband.bdProductPrice + "元"), $(".price").html(Number(broadband.bdProductPrice).toFixed(2) + "元"), 
            $(".payFeeMoney b").html(Number(broadband.bdProductPrice).toFixed(2)), $(".total b").html(Number(broadband.bdProductPrice).toFixed(2)), 
            $(".vip-card").hide(), "3" == e.activityType) {
                var d = e.limitAmount.split(",");
                if (null != d && 2 == d.length) {
                    var a = parseInt(d[0]), r = parseInt(d[1]);
                    a <= e.DiscntInfo.payAmount && r >= e.DiscntInfo.payAmount && $(".vip-card").show();
                }
            }
            validateSubmitCss() && ($(".payFeeMoney b").html(Number(broadband.bdProductPrice).toFixed(2)), 
            $(".total b").html(Number(broadband.bdProductPrice).toFixed(2)), $(".preferential b").text("0.00"), 
            payButton(!1)), broadband.secstate = e.secstate, computedHei();
        },
        error: function() {
            $(".checkAmount").hide(), $("#errormsg").html("系统超时,请重试！"), errormsgshow();
        }
    });
}, recognizeIsChange = function(e) {
    return judgeNoEmptyAndEqls(e.broadJudgeChargeStr, e.broadSelJudgeChargeStr) && !e.IsChange ? (broadband.selProductID = e.bdProductId, 
    broadband.selDiscntCode = e.selDiscntCode, broadband.broadSelJudgeChargeStr = e.bdProductId + e.selDiscntCode, 
    broadband.selNoChange = !1, !1) : judgeNoEmptyAndNoEqls(e.broadJudgeChargeStr, e.broadSelJudgeChargeStr) && e.IsChange ? (broadband.selProductID = e.selProductID, 
    broadband.selDiscntCode = e.selDiscntCode, broadband.broadSelJudgeChargeStr = e.selProductID + e.selDiscntCode, 
    broadband.selNoChange = !0, !0) : void 0;
}, showUserInfo = function(e) {
    $(".userInfoBasic .userInfo").html(e.BroadbandPackYearBean.custName), $(".userInfoBasic .userInfo").show();
}, proBack = function() {
    $(".product").show(), $(".outMessagesTips").show(), $(".proMessagesTipsContent").hide();
}, validateBroadbandAccount = function(e) {
    return /^([\w\-\.\@]){1,30}$/.test(e);
}, validateUnifiedAccount = function(e) {
    var d = e.replace("-", ""), a = /^([0-9]{1,30})$/;
    return a.test($.trim(d));
}, validateBDNumber = function() {
    judgeEmpty(getBdNumber()) || (validateNumType(broadband.bdNumber), validateAllParams()), 
    validateSubmitCss() && payButton(!0);
}, validateNumType = function() {
    if (returnNumAndNumType() || "08" != broadband.netCode) {
        if (!returnNumAndNumType() && "04" == broadband.netCode && !validateBroadbandAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确宽带账号！"), 
        void errormsgshow();
    } else if (!validateUnifiedAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确宽带编码！"), 
    void errormsgshow();
}, validateAllParams = function() {
    if (returnAll() || "08" != broadband.netCode) {
        if (!returnAll() && "04" == broadband.netCode && !validateBroadbandAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确的号码！"), 
        void errormsgshow();
    } else if (!validateUnifiedAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确的号码！"), 
    void errormsgshow();
}, validateAreaCode = function() {
    return broadband.subStrAreaCode = getAreaCode(broadband.broadProvCode, broadband.broadCityCode), 
    broadband.areaCode == broadband.subStrAreaCode ? !0 : void 0;
}, formatNumber = function(e) {
    if (judgeEmpty(broadband.netCode) || "08" != broadband.netCode) {
        if (!judgeEmpty(broadband.netCode) && "04" == broadband.netCode && !validateBroadbandAccount(e)) return $("#errormsg").html("请输入正确的号码!"), 
        errormsgshow(), !1;
    } else if (subStrNumber(e), !validateUnifiedAccount(e)) return $("#errormsg").html("请输入正确的号码!"), 
    errormsgshow(), !1;
    return !0;
}, validateQuerybdProdcut = function() {
    if (judgeEmpty(broadband.broadProvCode) && judgeEmpty(broadband.broadCityCode)) return $("#errormsg").html("请先选择地区!"), 
    void errormsgshow();
    if (judgeEmpty(broadband.bdNumber)) return $("#errormsg").html("号码不能空!"), void errormsgshow();
    if (judgeEmpty(broadband.netCode)) return $("#errormsg").html("请选择号码类型!"), void errormsgshow();
    if ("04" == broadband.netCode) {
        if (!validateBroadbandAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确的宽带账号!"), 
        void errormsgshow();
    } else if ("08" == broadband.netCode && !validateUnifiedAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确的宽带编码!"), 
    void errormsgshow();
    return !0;
}, validateParameters = function() {
    if (judgeEmpty($("input:visible").val())) return $("#errormsg").html("请填写正确的号码!"), 
    void errormsgshow();
    if (judgeEmpty(broadband.bdNumber)) return $("#errormsg").html("请点击“确定”按钮!"), errormsgshow(), 
    $("#idEnter").hide(), $("input:visible").addClass("broad-bold").removeClass("broad-nobold"), 
    void $(".del-num-broad,.confirm-btn-broad").show();
    if (judgeEmpty(broadband.netCode)) return $("#errormsg").html("请选择号码的类型!"), void errormsgshow();
    if (judgeEmpty(broadband.netCode) || "04" != broadband.netCode) {
        if (judgeEmpty(broadband.netCode) || "08" != broadband.netCode) {
            if (!judgeEmpty(broadband.netCode) && "10" == broadband.netCode && !validateBroadbandAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确格式的宽带账号/编码!"), 
            void errormsgshow();
        } else if (!validateUnifiedAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确格式的宽带编码!"), 
        void errormsgshow();
    } else if (!validateBroadbandAccount(broadband.bdNumber)) return $("#errormsg").html("请输入正确格式的宽带账号!"), 
    void errormsgshow();
    if (broadband.isHideCurPduc) {
        if (judgeEmpty(broadband.bdProductId)) return $("#errormsg").html("请选择要续费或要变更的产品!"), 
        void errormsgshow();
    } else if (judgeEmpty(broadband.selProductID)) return $("#errormsg").html("请选择要续费或要变更的产品!"), 
    void errormsgshow();
    return !0;
}, validateSubmitCss = function() {
    if (judgeEmpty(broadband.isOnline) || !broadband.isOnline) return !1;
    if (judgeEmpty(broadband.broadProvCode) && judgeEmpty(broadband.broadCityCode)) return !1;
    if (judgeEmpty(broadband.bdNumber)) return !1;
    if (judgeEmpty(broadband.netCode)) return !1;
    if ("04" == broadband.netCode) {
        if (!validateBroadbandAccount(broadband.bdNumber)) return !1;
    } else if ("08" == broadband.netCode && !validateUnifiedAccount(broadband.bdNumber)) return !1;
    return judgeEmpty(broadband.selProductID) ? !1 : !0;
}, subStrNumber = function(e) {
    !judgeEmpty(e) && /^(010)|(02[0-9])$/.test(e.substr(0, 3)) ? broadband.subStrAreaCode = e.substr(0, 3) : !judgeEmpty(e) && /^0\d{3}$/.test(e.substr(0, 4)) && (broadband.subStrAreaCode = e.substr(0, 4));
}, returnNumAndNumType = function() {
    return judgeEmpty(broadband.bdNumber) && judgeEmpty(broadband.netCode);
}, returnAll = function() {
    return judgeEmpty(broadband.bdNumber) && judgeEmpty(broadband.netCode) && judgeEmpty(broadband.broadProvCode) && judgeEmpty(broadband.broadCityCode);
}, returnAllParms = function() {
    return judgeEmpty(broadband.bdNumber) && judgeEmpty(broadband.netCode) && judgeEmpty(broadband.broadProvCode) && judgeEmpty(broadband.broadCityCode);
}, submitAllParms = function() {
    !returnAll();
}, back = function() {
    $(".warp").show(), $(".btn").show(), $(".footer").show(), $(".messagesTips").hide(), 
    $(".outInvoice").hide(), $("body").css("background", "#fcfcfc"), $(".close").hide();
}, showErrorTips = function() {
    $(".outError").show(), setTimeout(function() {
        $(".outError").fadeOut();
    }, 2e3);
}, proCityNumberType = function(e) {
    return "08" == broadband.netCode && returnAllParms(e) && (broadband.areaCode = getAreaCode(broadband.broadProvCode, broadband.broadCityCode), 
    subStrNumber(e), broadband.subStrAreaCode != broadband.areaCode) ? ($(".tips").html("输入的号码与地区不匹配！"), 
    void showErrorTips()) : void 0;
}, judgeNoEmptyAndEqls = function(e, d) {
    return !judgeEmpty(e) && !judgeEmpty(d) && e == d;
}, judgeNoEmptyAndNoEqls = function(e, d) {
    return !judgeEmpty(e) && !judgeEmpty(d) && e != d;
}, showPayfeeType = function() {
    return null == broadband.broadProvCode || "" == broadband.broadProvCode ? ($("#errormsg").html("请选择省份地市！").show(), 
    void errormsgshow()) : null == broadband.brandNumber || "" == broadband.brandNumber ? ($("#errormsg").html("请输入号码！").show(), 
    void errormsgshow()) : ($(".loadingdiv").show(), $.ajaxSettings.async = !1, ("" == broadband.broadLeftFee || null == broadband.broadLeftFee) && queryPhoneFeeInfo(), 
    $.ajaxSettings.async = !0, $(".loadingdiv").hide(), window.location.hash = "payfeeTypePage", 
    $(".rechargeDiv").hide(), 0 == parseInt(broadband.bdProductPrice) ? $(".payfeeTypeList li").eq(1).hide() : $(".payfeeTypeList li").eq(1).show(), 
    void $(".payfeeTypeDiv").show());
}, queryPhoneFeeInfo = function() {
    $.ajax({
        type: "post",
        data: {
            "commonBean.phoneNo": broadband.queryBroadCode,
            "commonBean.provinceCode": broadband.broadProvCode,
            "commonBean.cityCode": broadband.broadCityCode,
            "commonBean.areaCode": broadband.areaCode,
            "commonBean.channelType": ISWAP ? "307" : "308"
        },
        dataType: "json",
        url: serviceBankchargeURL + "broadbandPackYear/qryBroadFeeInfo"
    }).success(function(e) {
        "0000" == e.rspCode ? (broadband.broadLeftFee = e.feeinfo, $(".accountBalance b").html(e.feeinfo)) : $(".accountBalance b").html("查询失败");
    }).error(function() {
        $(".payfeeTypeHref").hide();
    });
}, confirmPayfeeType = function() {
    return broadband.bdProductPrice > broadband.broadLeftFee && "余额支付" == $(".payfeeType .cur").text() ? ($(".goPaypage").show(), 
    $("#errormsg").html("余额不足!").show(), void errormsgshow()) : ("余额支付" == $(".payfeeType .cur").text() && (broadband.isPayByBalance = !0), 
    window.location.hash = "", void $(".payfeeTypeName").text($(".payfeeType .cur").text()));
}, iptFocus = function(e) {
    $(e).val().length > 0 && "readonly" != $(e).attr("readonly") && $(e).parent(".ipt").find(".clearNum").show();
}, iptBlur = function(e) {
    $(e).parent(".ipt").find(".clearNum").hide();
}, iptKeyup = function(e) {
    $(e).parent(".ipt").find(".err").hide(), judgeEmpty($(e).val()) ? $(e).css("fontSize", ".32rem").parent().find(".clearNum").hide() : $(e).css("fontSize", ".4rem").parent().find(".clearNum").show(), 
    setRange(e, cardNumberSpace, 19);
    var d = $(e).val().length;
    if ("23" == d) {
        var a = $(e).val();
        $(e).attr("maxlength", 23);
        var r = (removeSpace(a), $(e).parent(".ipt").index());
        $(".ipts .ipt ").each(function(d, o) {
            return $(o).find(".rechargeIpt").val() == a && r != d ? ($(e).parent(".ipt").find(".err").html("请勿输入重复的充值卡密码").show(), 
            $(e).parent(".ipt").find(".ipt").find(".clearNum").show(), $(e).parent(".ipt").find(".ipt").find(".rechargeIpt").focus(), 
            !1) : void $(e).blur().parent().find(".clearNum").hide();
        });
    }
};

$(".clearNum").on("mousedown", function() {
    console.log("sdsdsd"), $(this).hide().prev().val("").css("fontSize", ".32rem"), 
    $(this).hide().prev().focus();
}), $(".removeList").on("click", function() {
    $(".ipts .ipt").length > 1 ? $(this).parent(".ipt").remove() : ($("#errormsg").html("请勿删除最后一个").show(), 
    errormsgshow());
}), $(".addIpt").on("click", function() {
    if ($(".err").is(":visible")) return !1;
    if ("10" == $(".ipts .ipt").length) return $("#errormsg").html("最多只能10条充值卡密码").show(), 
    errormsgshow(), !1;
    if (checkIpt()) {
        var e = $(".rechargeDiv .ipt").eq(0).clone(!0);
        e.find(".err").hide(), e.find(".rechargeIpt").removeAttr("readonly").val("").css({
            fontSize: ".32rem",
            color: "#000"
        }).end().find(".clearNum").hide().end().find(".rightNum").hide(), $(".ipts").append(e);
    }
});

var balancePayShow = function() {
    $(".goPaypage").hide(), "" != broadband.broadLeftFee && (broadband.bdProductPrice > broadband.broadLeftFee && ($(".goPaypage").show(), 
    $(".payfeeTypeDiv .btn span").css("background", "#ddd")), $(".remainSum").show());
}, onlinePayShow = function() {
    $(".remainSum").hide(), $(".payfeeTypeDiv .btn span").css("background", "#ff7745");
}, goPaypage = function() {
    $(".payfeeTypeDiv").hide(), $(".rechargeDiv").show(), $(".err").hide(), $(".rechargeDiv .number").text($("#broadNumber").val()), 
    $(".ipts .ipt:gt(0)").remove(), $(".ipts .ipt").eq(0).find(".rechargeIpt").val("").css({
        fontSize: ".32rem",
        color: "#000"
    }).removeAttr("readonly").end().find(".rightNum").hide();
}, showIdquery = function() {
    $(".idCheck").show();
}, showIdQueryProvince = function() {
    return $(".loadingdiv").is(":visible") ? !1 : ($(".idCheck").hide(), void (window.location.hash = "idQueryProvinceShow"));
}, fillIdQueryProvince = function() {
    var e = "";
    $.each(area.province, function(d, a) {
        e += '<li class="border-1px" onclick="idQueryProv(\'' + a.code + "','" + a.name + "');\">" + a.name + "</li>";
    }), $("#idQueryProvinceShow ul").html(e);
}, fillIdQueryBroadCity = function(e) {
    var d = "";
    $.each(e, function(e, a) {
        d += '<li class="border-1px" onclick="idQueryCity(\'' + a.name + "','" + a.pno + "','" + a.id + "');\">" + a.name + "</li>";
    }), $("#idQueryCityShow ul").html(d);
}, idQueryProv = function(e, d, a) {
    var r = getCitiesByPorv(e);
    fillIdQueryBroadCity(r), idquery.queryProvId = e, area.municipality.indexOf(e) >= 0 ? (a || window.history.back(), 
    $(".idqueryCity").val(d), idquery.queryCityId = r[0].id, $(".idCheck").show(), isGray()) : a || (window.location.hash = "idQueryCityShow");
}, idQueryCity = function(e, d, a, r) {
    area.municipality.indexOf(d) >= 0 && (e = ""), idquery.queryProvId = d, idquery.queryCityId = a, 
    $(".idqueryCity").val(getProvinceName(d) + " " + e), $(".idCheck").show(), isGray(), 
    r || (window.location.hash = "");
};

$(".closeIdCheck").click(function() {
    $(".idCheck").hide();
}), $(".closeRes,.closeBtn").on("click", function() {
    $(".checkRes").hide();
}), $(".checkBtn").click(function() {
    if ($(".loadingdiv").show(), judgeEmpty($(".idqueryCity").val())) return $("#errormsg").html("请选择安装地区").show(), 
    errormsgshow(), void $(".loadingdiv").hide();
    if (judgeEmpty($(".idNumber").val())) return $("#errormsg").html("请输入身份证号").show(), 
    errormsgshow(), void $(".loadingdiv").hide();
    if (!IdCardValidate($(".idNumber").val())) return $("#errormsg").html("身份证号码格式不正确").show(), 
    errormsgshow(), void $(".loadingdiv").hide();
    idquery.idCardNo = $(".idNumber").val();
    var action = "broadbandPackYear/getProductInfoByIdCard", param = {};
    param["BroadBandInfo.idCardNo"] = idquery.idCardNo, param["BroadBandInfo.accProvinceCode"] = idquery.queryProvId, 
    param["BroadBandInfo.accCityCode"] = idquery.queryCityId, $.ajax({
        url: serviceBankchargeURL + action,
        data: param,
        type: "post",
        dataType: "json",
        success: function(data) {
            if ($(".loadingdiv").hide(), $(".idCheck").hide(), "success" == data.out) {
                var list = eval(data.brodbandInfos);
                if (0 == list.length && ($("#errormsg").html("未查到信息!").show(), errormsgshow()), 
                null != list && 0 != list.length) {
                    var broadbandInfo = "<ul class='infos'><li class='info'><p class='title'>身份证号<span></span></p><span>：</span><span class='idNum'>";
                    broadbandInfo += idquery.idCardNo + "</span></li><li class='info'><p class='title'>姓名<span></span></p><span>：</span><span class='name'>", 
                    broadbandInfo += list[0].username + "</span></li><li class='info'><p class='title'>宽带信息<span></span></p><span>：</span></li></ul><ul class='queryLists'>";
                    for (var i = 0; i < list.length; i++) {
                        var info = list[i], areaCode = "", broadbandNo = "";
                        areaCode = getAreaCode(info.provinceCode, info.cityCode), broadbandNo = info.broadbandNo, 
                        broadbandInfo += "<li class='queryList'><div class='leftDiv'><p>", broadbandInfo += "<input type ='hidden' id='idqueryNo' value='" + broadbandNo + "'/><input type ='hidden' id='idqueryarea' value='" + areaCode + "'/><input type ='hidden' id='idqueryProv' value='" + info.provinceCode + "'/><input type ='hidden' id='idqueryCity' value='" + info.cityCode + "'/><span class='broadName'>宽带编码 : </span><span class='broadNum'>", 
                        broadbandInfo += info.broadbandNo + "</span></p><p>地址：" + info.addr + "</p></div><div class='rightDiv'><div class='RechargeBtn'>去充值</div></div></li>";
                    }
                    $(".queryRes").empty(), $(".queryRes").append(broadbandInfo), $(".checkRes").show();
                }
            } else "busy" == data.out ? ($("#errormsg").html("您的操作太频繁，请稍后再试！").show(), errormsgshow()) : ($("#errormsg").html(data.out).show(), 
            errormsgshow());
        },
        error: function() {
            $("#errormsg").html("系统繁忙，请稍候再试!").show(), errormsgshow();
        }
    });
});

var inputIdCard = function() {
    var e = $(".idNumber").val();
    judgeEmpty(e) ? ($(".idNumber").css("fontSize", ".3rem"), $(".delIdIcon").hide(), 
    $(".checkBtn").addClass("gray")) : ($(".idNumber").css("fontSize", ".34rem"), $(".delIdIcon").show(), 
    isGray());
};

$(".idNumber").bind("paste", function(e) {
    var d = void 0;
    d = window.clipboardData && window.clipboardData.getData ? window.clipboardData.getData("Text") : e.originalEvent.clipboardData.getData("Text"), 
    $(".idNumber").val(d), inputIdCard();
}), $(".delIdIcon").click(function() {
    $(".idNumber").val("").focus(), $(this).hide(), $(".checkBtn").addClass("gray"), 
    $(".idNumber").css("fontSize", ".3rem");
}), $(".queryRes").on("click", ".RechargeBtn", function() {
    var e = $(this).parents(".queryList").find("#idqueryNo").val(), d = $(this).parents(".queryList").find("#idqueryarea").val(), a = $(this).parents(".queryList").find("#idqueryProv").val(), r = $(this).parents(".queryList").find("#idqueryCity").val();
    charge.fixCode = d, broadband.areaCode = d;
    var o = !1;
    isPackYear ? $.each(broadband.broadNumberType, function(e, d) {
        "10" == d && (o = !0);
    }) : $.each(broadband.monthBroadNumberType, function(e, d) {
        "10" == d && (o = !0);
    }), o ? confirmNumType("10", !0) : confirmNumType("08", !0), o ? $("#broadNumber").val(e).addClass("broad-bold").removeClass("broad-nobold") : $("#broadBandPhone").val(e).addClass("broad-bold").removeClass("broad-nobold"), 
    $(".checkRes").hide(), $("#idEnter").hide(), charge.broadProvCode = a, charge.broadCityCode = r, 
    broadband.broadProvCode = a, broadband.broadCityCode = r, $(".loadingdiv").show(), 
    $(".sel-city").html("<i>" + $(".idqueryCity").val() + "</i>"), $.ajaxSettings.async = !1, 
    InitBroadband(), $.ajaxSettings.async = !0, confirmBroadBand(!1);
});var points = {};

points.isUsePointShow = !1, points.isPointShow = !1, points.userPoint = "", phone4G = {};

var checkAndConsumePoint = function(t) {
    if ($(".err-jf").removeClass("visible").addClass("unvisible"), charge.otherAmount = t ? charge.otherAmount : $("#otherAmount").val().replace(/[^\d\.]/g, ""), 
    judgeEmpty(points.userPoint)) return $(".pointsInput").val(""), $(".leftPoints").text("0"), 
    void $(".payFeeMoney b").html(charge.otherAmount);
    var e = $(".jfList .jfIpt"), o = e.val();
    o = parseFloat(Number(o)).toFixed(0), o = judgeEmpty(o) ? "0" : o.replace(/[^\d]/g, ""), 
    0 == o ? $(".jfList .jfIpt").val(o).css("color", "#999") : $(".jfList .jfIpt").val(o).css("color", "#333");
    var i;
    i = Number(!isBroadTab() || "04" != common.netCode && "05" != common.netCode ? charge.otherAmount : charge.broadAccountAmount), 
    Number(o) > Number(points.userPoint) && (o = Number(points.userPoint), $(".jfIpt").val(o.toFixed(0))), 
    Number(o) / 100 > i && (o = Number(100 * i), 0 == o ? $(".jfList .jfIpt").val(o).css("color", "#999") : $(".jfList .jfIpt").val(o).css("color", "#333"));
    var n = Number(o), s = Number(points.userPoint), a = parseFloat(Number(points.userPoint) / 100).toFixed(2), r = checkPoint(i, n, s);
    if ("true" == r) {
        var l = isNaN(i) ? "" : (accMul(i, 100) - n) / 100, u = parseFloat(Number(points.userPoint - n) / 100).toFixed(2), p = parseFloat(n / 100).toFixed(2);
        $(".leftPointsMoney").text(u), $(".pointsMoney").text(p), $(".leftPoints").text(points.userPoint - n), 
        $(".payFeeMoney b").text(l), $(".jfAbleMoney").text(a), $(".err-jf").text("积分抵扣" + p + "元还需支付" + l + "元").removeClass("unvisible").addClass("visible"), 
        0 >= n ? $(".mui-switch").prop("checked") ? judgeEmpty($("#otherAmount").val()) && (e.val(""), 
        $(".usejfList").show(), $(".pointsMoney").text("0"), $(".jfIpt").val("0"), $(".canUsePoint span b").css("color", "#ff7745")) : (e.val(""), 
        $(".usejfList").hide(), $(".pointsMoney").text(parseFloat(s / 100).toFixed(2)), 
        $(".canUsePoint span b").css("color", "#222")) : ($(".usejfList").show(), $(".canUsePoint span b").css("color", "#ff7745")), 
        charge.isJfjf && showPayList(paytypes_jf);
    }
}, checkIsFullPoint = function(t, e) {
    return charge.bankchargeamount || judgeEmpty(e) ? "0" : accMul(Number(t), 100) - Number(e) == 0 ? "1" : "0";
}, pointPhone4G = function() {
    if (!(!common.login4G || judgeEmpty(common.loginType) || common.phone_login.indexOf(common.loginType) < 0 && !isMobileTab())) {
        var t = isMobileTab() ? formateNumGetBasicPhone($("#number").val()) : charge.fixNum;
        t = isBroadTab() && "04" == common.netCode ? getBroadBandPhone() : t;
        var e = isMobileTab() ? "" : charge.fixCode, o = isMobileTab() ? "" : common.netCode;
        if (!(isMobileTab() && judgeEmpty(t) || isFixedTab() && judgeEmpty(charge.fixPhone) || isBroadTab() && judgeEmpty(getBroadBandPhone()))) {
            var i = judgeEmpty(e) ? t : e + "-" + t;
            return judgeEmpty(phone4G[i]) ? void $.ajax({
                url: servicePayFeeUrl + "pointJudge/pointValid",
                data: {
                    numberType: o,
                    "commonBean.phoneNo": t,
                    "commonBean.areaCode": e
                },
                type: "get",
                dataType: "json",
                success: function(t) {
                    return t.limit || !t.isSameNo ? (phone4G[i] = !1, $(".integral-list").removeClass("amount-cur"), 
                    $(".integral-list .true_money").css({
                        height: "1rem",
                        lineHeight: "1rem"
                    }), $(".integral-list  .original_price").hide(), void $(".integral-list").addClass("gray-amount-list")) : (phone4G[i] = t.is4GNo, 
                    void showPoint(phone4G[i]));
                },
                error: function() {
                    points.isPointShow = !1;
                }
            }) : (showPoint(phone4G[i]), $(".integral-list .true_money").css({
                height: ".68rem",
                lineHeight: ".68rem"
            }), $(".integral-list .original_price").show(), void $(".integral-list").removeClass("gray-amount-list"));
        }
    }
}, showPoint = function(t) {
    return charge.isJfjf = !1, $(".integral-list").addClass("gray-amount-list"), charge.isNotAllowPoint ? (removePointPay(), 
    void $(".integral-list").removeClass("amount-cur")) : t && common.login4G || !points.isPointShow ? (points.isPointShow && checkAndConsumePoint(), 
    void isPointShow()) : (removePointPay(), $("#errormsg").html("积分交费仅支持4G号码").show(), 
    errormsgshow(), void $(".integral-list").removeClass("amount-cur"));
}, removePointPay = function() {
    charge.isJfjf = !1, $(".integral-list").addClass("gray-amount-list"), points.isPointShow = !1, 
    $(".jfAmout").hide();
}, checkPoint = function(t, e, o) {
    if (e > o) return $(".pointsInput").val(""), $(".jfList .jfIpt").val(""), $(".payFeeMoney b").html(t), 
    $(".leftPointsMoney").text((Number(points.userPoint) / 100).toFixed(2)), $(".pointsMoney").text("0.00"), 
    $(".leftPoints").text(points.userPoint), $(".err-jf").html("输入积分数超过可用积分数，请重新输入！").removeClass("unvisible").addClass("visible"), 
    "输入积分数超过可用积分数，请重新输入！";
    if (0 > mustpayFeeInfo && e > accMul(t, 100)) {
        var i = accMul(t, 100), n = parseFloat(Number(points.userPoint - i) / 100).toFixed(2), s = parseFloat(Number(i) / 100).toFixed(2);
        return $(".pointsInput").val(i > 0 ? i : ""), $(".jfList .jfIpt").val(i), $(".payFeeMoney b").text("0"), 
        $(".leftPointsMoney").text(n), $(".pointsMoney").text(s), $(".leftPoints").text(points.userPoint - i), 
        $("#pointNum").val(i), $(".err-jf").text("积分抵扣" + s + "元还需支付0元").removeClass("unvisible").addClass("visible"), 
        charge.isJfjf && showPayList(paytypes_jf), "";
    }
    return "true";
}, checkIptPoint = function(t, e) {
    if (e >= accMul(t, 100)) {
        var o = accMul(t, 100), i = parseFloat(Number(points.userPoint - o) / 100).toFixed(2), n = parseFloat(Number(o) / 100).toFixed(2);
        return $(".pointsInput").val(o > 0 ? o : ""), 0 == o ? $(".jfList .jfIpt").val(o).css("color", "#999") : $(".jfList .jfIpt").val(o).css("color", "#333"), 
        $(".payFeeMoney b").text("0"), $(".leftPointsMoney").text(i), $(".pointsMoney").text(n), 
        $(".leftPoints").text(points.userPoint - o), $("#pointNum").val(o), charge.isJfjf && showPayList(paytypes_jf), 
        "";
    }
    if (e < accMul(t, 100) && $(".mui-switch").prop("checked")) {
        var o = accMul(t, 100), i = parseFloat(Number(points.userPoint - o) / 100).toFixed(2), n = parseFloat(Number(e) / 100).toFixed(2);
        $(".pointsInput").val(o > 0 ? o : ""), 0 == e ? $(".jfList .jfIpt").val(e).css("color", "#999") : $(".jfList .jfIpt").val(e).css("color", "#333"), 
        $(".payFeeMoney b").text(parseFloat(Number(t) - Number(n)).toFixed(2)), $(".pointsMoney").text(n), 
        $(".leftPoints").text(points.userPoint - o), $("#pointNum").val(o), showPayList(paytypes_jf);
    }
    return "true";
}, isPointShow = function() {
    return judgeEmpty(points.userPoint) ? void $.ajax({
        type: "post",
        url: servicePayFeeUrl + "pointJudge/getUserPoint",
        data: {
            "commonBean.channelType": CHANNEL_TYPE
        },
        dataType: "json"
    }).success(function(t) {
        if ($(".loadingdiv").hide(), "success" == t.out) {
            if (points.userPoint = t.userPoint, $(".pointsInput").val(""), $(".jifenAbleNum").html(t.userPoint), 
            $(".leftPoints").text(points.userPoint), points.isPointShow && pointMaskShow(), 
            common.loginPhoneNo != getChargePhone()) return;
            charge.isJfjf = !0, $(".integral-list").removeClass("gray-amount-list"), showJfCon(), 
            $(".integral-list .true_money").css({
                height: ".68rem",
                lineHeight: ".68rem"
            }), $(".integral-list .original_price").show(), $(".integral-list").removeClass("gray-amount-list");
        } else "busy" == t.out;
        points.isPointShow = !1;
    }).error(function() {
        points.isPointShow = !1;
    }) : (charge.isJfjf = !0, $(".integral-list").removeClass("gray-amount-list"), void (points.isPointShow && (pointMaskShow(), 
    checkAndConsumePoint(), points.isPointShow = !1)));
}, pointMaskShow = function() {
    $(".amount-cur").removeClass("amount-cur"), $(".cardlist .integral-list").addClass("amount-cur"), 
    charge.bankchargeamount = !1, $(".otherAmoutIpt").focus();
