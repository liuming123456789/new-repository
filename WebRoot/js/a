            if (6 == e.activity_type) return "<b class='sendVip'></b>";
            if (8 == e.activity_type) return "<b class='yxIcon'></b>";
        }
    }
    return "albkyd" != $("#channelKey").val() || "03" != e.cardValCode && "04" != e.cardValCode ? "" : "<b class='recommend'></b>";
}, checkSelectOff = function(e) {
