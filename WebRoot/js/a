    $(".bankcharge").show(), $(".page-body-box").show(), judgeEmpty($("#broadBandfix").val()) ? $("#broadBandfix").css("fontSize", ".32rem") : $("#broadBandfix").css({
        fontSize: ".4rem",
        padding: "0.2rem 0 0",
        "text-align": "center"
    }), $("#fixNumInput").css("padding", ".2rem 0 0")), n.each(function() {
        $(this).attr("id") == i && ($(this).show().siblings("div[data-role='page']").hide(), 
        $(window).scrollTop(0));
    }), $(".error-tips").hide(), "08" == common.netCode && $(".otherInputBro").hide();
}

function isInclude(e, i) {
    for (var n = i.length, s = 0; n > s; s++) if (e === i[s]) return !0;
    return !1;
}

function GetQueryString(e) {
    var i = new RegExp("(^|&)" + e + "=([^&]*)(&|$)"), n = window.location.search.substr(1).match(i);
    return null != n ? unescape(n[2]) : null;
}

