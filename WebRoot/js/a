}, InitBroadband = function() {
    $(".loadingdiv").show();
    var e = "", d = "";
    judgeEmpty(broadband.broadProvCode) ? (e = charge.broadProvCode, d = charge.broadCityCode, 
    broadband.broadProvCode = e, broadband.broadCityCode = d) : (e = broadband.broadProvCode, 
    d = broadband.broadCityCode), $.ajax({
        data: {
            proId: e,
            code: d
        },
        dataType: "jsonp",
        jsonp: "callback",
        jsonpCallback: "broadBand",
        url: serviceBankchargeURL + (ISWAP ? "broadbandPackYear/initBroadBand" : "broadbandPackYear/initBroadBand")
    }).success(function(e) {
        if ($(".loadingdiv").hide(), judgeEmpty(e)) {
            if (judgeEmpty(e)) return $(".loadingdiv").hide(), $("#errormsg").html("系统异常，请稍后再试！"), 
            void errormsgshow();
        } else broadband.isOnline = !0, broadband.isHideCurPduc = e.isHideCurProduct, judgeOnlineNewBroadType(broadband.broadProvCode) ? (broadband.broadNumberType = [ "10" ], 
        showbroadNumberType()) : (broadband.broadNumberType = e.BROADTELTYPE.split(","), 
        showbroadNumberType());
        computedHei();
    }).error(function() {
        $(".loadingdiv").hide(), $("#errormsg").html("系统异常，请稍后再试！"), errormsgshow();
    });
}, InputConfirm = function() {
    return broadband.bdNumber = jQuery.trim("宽带账号" == $("#netType").text() ? $("#broadBandAccount").val().replace("-", "") : "宽带编码" == $("#netType").text() ? $("#broadBandPhone").val().replace("-", "") : $("#broadNumber").val().replace("-", "")), 
    judgeEmpty(jQuery.trim($("#broadBandPhone").val())) || $(".del-num").hide(), $(".del-num,.confirm-btn").hide(), 
    broadband.broadLeftFee = "", onlinePayShow(), $(".payfeeTypeHref").hide(), judgeEmpty(broadband.broadProvCode) || judgeEmpty(broadband.broadCityCode) ? ($("#errormsg").html("请选择省份地市！"), 
    void errormsgshow()) : judgeEmpty(broadband.isOnline) && !broadband.isOnline ? ($("#errormsg").html("该省份的宽带趸交没有上线！"), 
    void errormsgshow()) : judgeEmpty(broadband.netCode) ? ($("#errormsg").html("请选择号码类型！"), 
    void errormsgshow()) : judgeEmpty(jQuery.trim(broadband.bdNumber)) ? ($("#errormsg").html("请输入号码！"), 
    void errormsgshow()) : void queryProduction();
}, pleaseChoose = function() {
    return $(".loadingdiv").is(":visible") ? !1 : (ismonthquery && monthproductHidden && $(".current").hide(), 
    page = 2, void (validateQuerybdProdcut() && ($("#messageTipsDiv").hide(), $(".productionPage").show(), 
