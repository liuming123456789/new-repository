}, createCashVoucherPage = function() {
    var e = isMobileTab() ? cards.offerRateList : isFixedTab() ? cards.offerRateFixList : cards.offerRateBroadList, a = judgeEmpty(cards.selectedOfferRateId) ? "smart" : cards.selectedOfferRateId;
