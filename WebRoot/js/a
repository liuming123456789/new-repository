    $(".userInfoBasic .userInfo").hide(), $(".fixed .bdNumType").html("请选择"), broadband.bdProductId = "", 
    broadband.bdProductPrice = "", broadband.brandSpeed = "", broadband.startEnable = "", 
    broadband.bdDiscntName = "", broadband.accessType = "", broadband.selDiscntCode = "", 
    broadband.bdNumber = "", broadband.IsChange = !1, $(".submit b").html(""), $(".submit").css("background-color", "#ddd"), 
    validateSubmitCss() && payButton(!0), computedHei();
}, showNormalNumberType = function(e) {
    broadband.monthBroadNumberType = "";
    var d = $("#broadnumberType").next().find("li"), a = [], r = [];
    if (broadband.monthBroadNumberType = e ? [ "10", "03" ] : [ "08", "04", "03" ], 
    $.each(d, function(e, d) {
        $(d).hide(), $.each(broadband.monthBroadNumberType, function(o, n) {
            $(d).attr("value") == n && ($(d).show(), a.push($(d).text()), r.push(e));
        });
    }), $.inArray($(".numberType .cur").text(), a) < 0) {
        var o = d.eq(r[0]).val();
        confirmNumType(o, !0), $("#netType").html(a[0]);
    }
}, showbroadNumberType = function(e) {
    var d = "", a = [ "8", "4", "10", "3" ], r = $("#broadnumberType").next().find("li"), o = [], n = [];
    $.each(r, function(e, d) {
        $(d).hide();
    }), $.each(a, function(e, a) {
        broadband.broadNumberType.indexOf(a) > -1 && ("3" == a ? d = "04" : "4" == a ? d = "08" : "10" == a && (d = "10"), 
        $.each(r, function(e, a) {
            $(a).attr("value") == d && ($(a).show(), o.push($(a).text()), n.push(e));
        }));
    }), ($.inArray($(".numberType .cur").text(), o) < 0 || e) && (r.eq(n[0]).addClass("cur").siblings().removeClass("cur"), 
    r.eq(n[0]).click(), $("#netType").html(o[0]));
}, judgeOnlineNewBroadType = function(e) {
    if (judgeEmpty(onlineBroadNewTypeProvince)) return !0;
    for (var d = 0; d < onlineBroadNewTypeProvince.length; d++) if (e == onlineBroadNewTypeProvince[d]) return !0;
    return !1;
