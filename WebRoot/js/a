        historyNum.fixHistoryNum[o].CITY_NAME = getCityName(i[o].provinceCode, i[o].cityCode);
    }
    $("#fixHisNumDiv ul").html(createHis(historyNum.fixHistoryNum, "chooseFixHistory", "fix")), 
    $("#fixPhone").unbind("blur"), $("#fixPhone").bind("blur", function() {
        $("#fixHisNumDiv").hide();
    }), $("#fixHisNumDiv").mouseover(function() {
        $("#fixPhone").unbind("blur");
    }), $("#fixHisNumDiv").mouseout(function() {
        $("#fixPhone").bind("blur", function() {
            $("#fixHisNumDiv").hide();
        });
    });
}, fillMobHistory = function(i) {
