};function pageSerTypeMap() {
    var e = new Map();
    return e.put("mobbuycard", "mobbuycard"), e.put("mobbankcharge", "mobbankcharge"), 
    e.put("fixbankcharge", "fixbankcharge"), e.put("broadbandbankcharge", "broadbandbankcharge"), 
    e.put("recharge", "recharge"), e.put("confirm", "confirm"), e.put("rechargeFinish", "rechargeFinish"), 
    e.put("mobquery", "mobquery"), e.put("actionpayfeewrite", "actionpayfeewrite"), 
    e.put("actionpayfeeconfirm", "actionpayfeeconfirm"), e;
}

function pageCodeMapping() {
    var e = new Map();
    return e.put("mobbuycard", "MOBBUYCARD"), e.put("mobbankcharge", "MOBILEANKCHARGEWRITE"), 
    e.put("fixbankcharge", "FIXBANKCHARGEWRITE"), e.put("broadbandbankcharge", "BANDBANKCHARGEWRITE"), 
    e.put("recharge", "MOBRECHARGEWRITE"), e.put("confirm", "MOBRECHARGECONFIRM"), e.put("rechargeFinish", "MOBRECHARGEFINISH"), 
    e.put("mobquery", "MOBQUERYWRITE"), e.put("actionpayfeewrite", "MOBACITVITYPAYFEEWRITE"), 
    e.put("actionpayfeeconfirm", "MOBACITVITYPAYFEECONFIRM"), e;
}

function getAddr(actionName, url) {
    if (!messageTips.get980) {
        messageTips.get980 = !0;
        var params = {};
        params.pageCode = actionName.toUpperCase(), params.provinceCode = "098";
        var fileName = "";
        $.getJSON(url, params, function(json) {
            fileName = json.fileName;
            var pageContentList = "", warmTips = "";
            $.getScript(fileName, function() {
                if ("undefined" != typeof messageTipfiledata && "" != messageTipfiledata) {
                    var messagetipsdata = eval(messageTipfiledata);
                    $.each(messagetipsdata, function(e, a) {
                        contextIndex = e + 1 + messageTips.count, scroll.messagetips.push(scrollFormatTips.replace("scrollContent", a.pageContent)), 
                        warmTips += "<p>" + contextIndex + "、" + a.pageContent + "</p>";
                    }), $(".warmContent").append(warmTips), $("#messageTipsf").show(), resetScroll();
                }
                messageTips.get980 = !1;
            });
        });
    }
}

function hideProvMtips() {
    $("#showProv").show(), $("#provMtipsDiv").hide(), $("#hideProv").hide();
}

function Map() {
    this.container = new Object();
}

function openPrompt() {
    var e = $(".openPrompt");
    e.remove(), $("#messageTipsDiv").append(" <span class='cBlue right packupPrompt' onclick='packupPrompt()'>收起</span>"), 
    $(".pointCon p").show();
}

function packupPrompt() {
    var e = $(".packupPrompt");
    e.remove(), $("#messageTipsDiv").append(" <span class='cBlue right openPrompt' onclick='openPrompt()'>更多</span>"), 
    $(".pointCon p").eq(1).nextAll("p").hide();
}

var messageTips = {};

messageTips.count = 0, messageTips.show = 0, messageTips.inputNumProvince = "", 
messageTips.servieType = "", messageTips.actionName = "", messageTips.get980 = !1;

var fillMessagetips = function(serviceType) {
    var pageSerType = pageSerTypeMap().get(serviceType);
    void 0 != pageSerType && (messageTips.actionName = pageCodeMapping().get(pageSerType));
    var fileName = "";
    if ("welcome" != messageTips.actionName) {
        if (void 0 == messageTips.actionName || "" == messageTips.actionName) return;
        var url = messagetipwebroot + "messageTips.js?callback=?", params = {};
        params.pageCode = messageTips.actionName.toUpperCase(), "undefined" != typeof messageTips.inputNumProvince && "undefined" != typeof common.loginProvince && "undefined" != typeof common.isLogin && (common.isLogin && "" != common.loginProvince && null != common.loginProvince ? params.provinceCode = common.loginProvince : "" != messageTips.inputNumProvince && null != messageTips.inputNumProvince && (params.provinceCode = messageTips.inputNumProvince));
        var contextIndex = 0;
        messageTips.count = 0, $.getJSON(url, params, function(json) {
            fileName = json.fileName;
            var pageContentList = "", warmTips = "";
            $.ajax({
                url: fileName,
                dataType: "script",
                success: function() {
                    if ($(".warmContent").empty(), scroll.messagetips = [], messageTips.count = 0, contextIndex = 0, 
                    messageTips.show = 0, "undefined" != typeof messageTipfiledata) if ("" != messageTipfiledata) {
                        var messagetipsdata = eval(messageTipfiledata);
                        $.each(messagetipsdata, function(e, a) {
                            contextIndex = e + 1, messageTips.count = e + 1, scroll.messagetips.push(scrollFormatTips.replace("scrollContent", a.pageContent)), 
                            warmTips += "<p>" + contextIndex + "、" + a.pageContent + "</p>";
                        }), $(".warmContent").append(warmTips), $("#messageTipsf").show(), $(".warmTip").show(), 
                        flag = contextIndex;
                    } else $(".warmTip").hide(), messageTips.show = 0, $("#tipContent").empty(), messageTips.count = 0, 
                    contextIndex = 0;
                    resetScroll();
                },
                error: function() {}
            });
        });
    }
};

Map.prototype.put = function(e, a) {
    this.container[e] = a;
}, Map.prototype.get = function(e) {
    return this.container[e];
}, Map.prototype.keySet = function() {
    var e = new Array(), a = 0;
    for (var n in this.container) "extend" != n && (e[a] = n, a++);
    return e;
}, Map.prototype.size = function() {
    var e = 0;
    for (var a in this.container) "extend" != a && e++;
    return e;
}, Map.prototype.remove = function(e) {
    delete this.container[e];
}, Map.prototype.toString = function() {
    for (var e = "", a = 0, n = this.keySet(), t = n.length; t > a; a++) e = e + n[a] + "=" + this.container[n[a]] + ";\n";
    return e;
}, $(".pointCon p").eq(1).nextAll("p").hide();function confirmBroadBandli(e) {
    var d = $(".numberType .cur").attr("value");
    if ($("#idEnter").hide(), confirmNumType(d, e), 1 == isPackYear) {
        $(".payfeeTypeHref").hide();
        for (var a = !1, r = 0; r < broadband.broadNumberType.length; r++) ("4" == broadband.broadNumberType[r] || "10" == broadband.broadNumberType[r]) && (a = !0);
        "10" != d && "04" != d && "08" != d || 1 != a || $("#idEnter").show();
    }
    "04" == d && "" == $(".otherBrandInput").val() && ($(".amount-list-broad").removeClass("amount-cur"), 
    $(".payFeeMoney b").html("0.00元"), $(".total b").html("0.00"));
}

function broadBlur() {
    if (1 == isPackYear && !$(".confirm-btn-broad").is(":visible")) {
        for (var e = !1, d = $(".numberType .cur").attr("value"), a = 0; a < broadband.broadNumberType.length; a++) ("4" == broadband.broadNumberType[a] || "10" == broadband.broadNumberType[a]) && (e = !0);
        "10" != d && "04" != d && "08" != d || 1 != e || $("#idEnter").show();
    }
}

