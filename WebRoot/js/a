}, removeSpace = function(e) {
    for (e = e.replace(/^\s\s*/, ""), ws = /\s/, i = e.length; ws.test(e.charAt(--i)); ) ;
    return e.slice(0, i + 1);
}, GetRequest = function(e) {
    var n = location.search;
    if (-1 != n.indexOf("?")) for (var o = n.substr(1), t = o.split("&"), i = 0; i < t.length; i += 1) if (e == t[i].split("=")[0]) return t[i].split("=")[1];
