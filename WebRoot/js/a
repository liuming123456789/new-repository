});

var choseIcon = function() {
    $(this).parent().find("i").click();
};

$(".close_div").on("click", function() {
    $(".shade").hide(), $(".agreeMentCon").hide(), $(".footer").show(), ModalHelper.beforeClose();
}), $(".agreement b").on("click", function() {
    cancelBubble(), $(".shade").show(), $(".agreeMentCon").show(), $(".footer").hide(), 
    ModalHelper.afterOpen();
});

var balanceUsed = function() {
    $(".balance i").toggleClass("bal-checked");
}, isAgree = function() {
    $(".agreement i").toggleClass("bal-checked");
};