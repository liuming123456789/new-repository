        var o = n.substr(1), t = o.split(e), i = t[1].substr(1);
        return i;
    }
}, changeTwoDecimal_f = function(e) {
    var n = parseFloat(e);
    if (isNaN(n)) return !1;
    var n = Math.round(100 * e) / 100, o = n.toString(), t = o.indexOf(".");
    for (0 > t && (t = o.length, o += "."); o.length <= t + 2; ) o += "0";
    return o;
