function clientSetContactPhoneNum(info) {
    if (isMobileTab() && !judgeEmpty(info) && "[]" != info ? clearPhoneNumber() : isFixedTab() && !judgeEmpty(info) && "[]" != info && clearFixNumber(), 
    eval("var phoneInfos=" + info), $("#historyNumDiv,#fixHisNumDiv,#broadHisNumDiv").hide(), 
    phoneInfos.telinfo.length > 1) {
        var addressInfos = "";
        $.each(phoneInfos.telinfo, function(e, o) {
            addressInfos += "<li onclick='chooseAddress(\"" + o.phone + '","' + o.name + "\")'><label>" + o.phone + "</label><span>" + o.name + "</span></li>";
        }), $("#addressBookUl").html(addressInfos), $("#addressBookDiv").show(), $("#number").unbind("blur"), 
        $("#number").bind("blur", function() {
            numberBlur();
        });
    } else chooseAddress(phoneInfos.telinfo[0].phone, phoneInfos.telinfo[0].name, "bankcharge");
}

function chooseAddress(e, o) {
    o = judgeEmpty(o) ? "" : o.trim();
    judgeEmpty(o) || e == o || /^(0\d{2,3}-*\d{7,8})$/.test(o);
    if (e = formateNumGetBasicPhone(e), isBroadTab()) return "03" == common.netCode ? ($("#broadBandfix").val(e), 
    $("#broadBandfix").attr("class", "num-ipt-bold").css("color", "#333")) : "04" == common.netCode || "05" == common.netCode ? ($("#broadBandAccount").val(e), 
    $("#broadBandAccount").attr("class", "num-ipt-bold-s").css("color", "#333")) : "08" == common.netCode && ($("#broadBandPhone").val(e), 
    $("#broadBandPhone").attr("class", "num-ipt-bold").css("color", "#333")), $("#addressBookDiv").hide(), 
    $("#historyNumDiv").hide(), $("#fixHisNumDiv").hide(), $("#broadHisNumDiv").hide(), 
    void $("#addressBookUl").html("");
    if (!common.phoneRex.test(e) && !common.fixedRex.test(e) && !/^(\d{7,8})$/.test(e)) return void $("#phoneError").html("请输入正确的交费号码");
    var i = e.replace(/[^\d()+]/g, "").substr(0, 11).replace(/(^\d{3}|\d{4})\B/g, "$1 "), n = i;
    if (isMobileTab()) common.phoneRex.test(e) ? ($("#number").val(n), phoneSure(!0), 
    $("#number").blur()) : ($("#errormsg").html("请输入正确的手机号码").show(), errormsgshow()); else if (isFixedTab()) {
        if (common.fixedRex.test(e) || /^(\d{7,8})$/.test(e) || ($("#errormsg").html("请输入正确的固话号码").show(), 
        errormsgshow()), /^(010)|(02[0-9])$/.test(e.substr(0, 3))) {
            var s = e.substr(0, 3), r = e.substr(3);
            $("#fixCode").length > 0 ? ($("#fixCode").val(s), $("#fixPhone").val(r)) : $("#fixPhone").val(e);
        } else if (/^0\d{3}$/.test(e.substr(0, 4))) {
            var s = e.substr(0, 4), r = e.substr(4);
            $("#fixCode").length > 0 ? ($("#fixCode").val(s), $("#fixPhone").val(r)) : $("#fixPhone").val(e);
        } else /^(\d{7,8})$/.test(e) && $("#fixPhone").val(e);
        confirmFix(!1);
    }
    if ($("#fixPhone").blur(), $("#fixCode").blur(), isIntimatePay()) if (isInitMobileTab()) {
        if (common.phoneRex.test(e)) {
            $("#number").val(n), phoneSure(!0), $("#number").blur(), $("#number").attr("class", "num-ipt-bold").css("color", "#333");
            var e = $("#number").val();
            judgeEmpty(e) || $.getJSON(nginxRequstURL + "getArea/init?callback=?", {
                "commonBean.phoneNo": formateNumGetBasicPhone(e)
            }, function(o) {
                if (charge.provinceCode = o.prov.split("X")[0], charge.cityCode = o.prov.split("X")[1], 
                void 0 == o.prov.split("X")[2]) return $("#errormsg").html("对不起,暂不支持该号码交费！").show(), 
                void errormsgshow();
                var i = area.municipality.indexOf(charge.provinceCode) >= 0 ? "" : getProvinceName(charge.provinceCode), n = getCityName(charge.provinceCode, charge.cityCode);
                getCustomInfo(e.replace(/\s+/g, ""), i, n);
            });
        }
    } else if (isInitFixedTab()) {
        if (common.fixedRex.test(e) || /^(\d{7,8})$/.test(e) || ($("#errormsg").html("请输入正确的固话号码").show(), 
        errormsgshow()), /^(010)|(02[0-9])$/.test(e.substr(0, 3))) {
            var s = e.substr(0, 3), r = e.substr(3);
            $("#fixCode").length > 0 ? ($("#fixCode").val(s), $("#fixPhone").val(r)) : $("#fixPhone").val(e);
        } else if (/^0\d{3}$/.test(e.substr(0, 4))) {
            var s = e.substr(0, 4), r = e.substr(4);
            $("#fixCode").length > 0 ? ($("#fixCode").val(s), $("#fixPhone").val(r)) : $("#fixPhone").val(e);
        } else /^(\d{7,8})$/.test(e) && $("#fixPhone").val(e);
        confirmFix(!1), $("#fixPhone").blur();
    }
    $("#addressBookDiv,#historyNumDiv,#fixHisNumDiv,#broadHisNumDiv").hide(), $("#addressBookUl").html("");
}

var clickid = "";var historyNum = {};

