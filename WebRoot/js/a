}, makeCardStr = function(e, a, r, t) {
    var s = e.offerRateId;
    judgeEmpty(cards.selectedOfferRateId) || "smart" == cards.selectedOfferRateId || cards.selectedOfferRateId == s || (s = cards.selectedOfferRateId);
    for (var i = [ "", 1, "", "" ], o = "", c = judgeEmpty(e.allOfferRate) ? [] : JSON.parse(e.allOfferRate), d = 0; d < c.length; d++) {
        var n = !1;
        for (var l in c[d]) c[d][l][0] == s && (i = c[d][l], o = l, n = !0);
        if (n) break;
    }
    if ("08" == o) {
        var f = "";
        return "<a onclick='select(" + calOfferPriceStr(o, i[1], e.cardValue) + "," + e.cardValue + ',"' + e.cardValCode + '","' + i[1] + '","' + o + '","' + i[0] + '",' + e.allOfferRate + ');\' href="javascript:void(0)" data-value="' + calOfferPriceStr(o, i[1], e.cardValue) + '" cardValCode="' + e.cardValCode + '" cardValue="' + e.cardValue + '" class="amount-list ' + f + ((t + 1) % 3 != 0 ? " mr5" : "") + '">' + a + '<p class="true_money">' + e.cardValueStr + '元</p><p class="original_price">售价：' + calOfferPriceStr(o, i[1], e.cardValue) + '元</p><b class="coupon-icon "><b class="coupon-sale">减' + 100 * i[1] / 1e4 + "元</b></b></a>";
    }
    return normalCardStr(e, a, r, i[1], o, i[0], t);
}, normalCardStr = function(e, a, r, t, s, i, o) {
    var c = (o + 1) % 3 != 0, d = "", n = judgeEmpty(e.allOfferRate) ? "[]" : e.allOfferRate;
    return 1 > t ? "2018051101" == charge.offerateId ? "<a onclick='select(" + calOfferPriceStr(s, t, e.cardValue) + "," + e.cardValue + ',"' + e.cardValCode + '","' + t + '","' + s + '","' + i + '",' + n + ');\' href="javascript:void(0)" data-value="' + calOfferPriceStr(s, t, e.cardValue) + '" cardValCode="' + e.cardValCode + '" cardValue="' + e.cardValue + '" class="amount-list ' + d + (c ? " mr5" : "") + '"><b class="activityIcon"></b>' + a + '<p class="true_money">' + 100 * t * e.cardValueStr / 100 + '元</p><p class="original_price">售价：' + Number(e.cardValueStr).toFixed(2) + "元</p></a>" : "2018051102" == charge.offerateId ? "<a onclick='select(" + calOfferPriceStr(s, t, e.cardValue) + "," + e.cardValue + ',"' + e.cardValCode + '","' + t + '","' + s + '","' + i + '",' + n + ');\' href="javascript:void(0)" data-value="' + calOfferPriceStr(s, t, e.cardValue) + '" cardValCode="' + e.cardValCode + '" cardValue="' + e.cardValue + '" class="amount-list ' + d + (c ? " mr5" : "") + '"><b class="activityIcon_15"></b>' + a + '<p class="true_money">' + 100 * t * e.cardValueStr / 100 + '元</p><p class="original_price">售价：' + Number(e.cardValueStr).toFixed(2) + "元</p></a>" : "<a onclick='select(" + calOfferPriceStr(s, t, e.cardValue) + "," + e.cardValue + ',"' + e.cardValCode + '","' + t + '","' + s + '","' + i + '",' + n + ');\' href="javascript:void(0)" data-value="' + calOfferPriceStr(s, t, e.cardValue) + '" cardValCode="' + e.cardValCode + '" cardValue="' + e.cardValue + '" class="amount-list ' + d + (c ? " mr5" : "") + '"><b class="coupon-icon "><b class="coupon-sale">' + 1e3 * t / 100 + "折</b></b>" + a + '<p class="true_money">' + e.cardValueStr + '元</p><p class="original_price">售价：' + calOfferPriceStr(s, t, e.cardValue) + "元</p></a>" : "<a onclick='select(" + calOfferPriceStr(s, t, e.cardValue) + "," + e.cardValue + ',"' + e.cardValCode + '","' + t + '","' + s + '","' + i + '",' + n + ');\' href="javascript:void(0)" data-value="' + calOfferPriceStr(s, t, e.cardValue) + '" cardValCode="' + e.cardValCode + '" cardValue="' + e.cardValue + '" class="amount-list ' + d + (c ? " mr5" : "") + '">' + a + '<p class="true_money">' + e.cardValueStr + '元</p><p class="original_price">售价：' + calOfferPriceStr(s, t, e.cardValue) + "元</p></a>";
