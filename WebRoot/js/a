    "01" == a && changePayShow(), charge.broadAccountAmount = judgeEmpty(charge.broadAccountAmount) ? "" : charge.broadAccountAmount, 
    $(".otherBrandInput").val(charge.broadAccountAmount), removePointPay(), $("#broadBandAccount").addClass("broad-nobold").removeClass("broad-bold")) : "08" == e ? ($("#broadNumber").hide(), 
    $("#fixNumInput").hide(), $(".septal-line").hide(), $(".broad").addClass("broadCardList"), 
    $(".otherBrandInput").val(""), $(".otherBrandInput").removeClass("amount-ipt-blod"), 
    $("#other").removeClass("amount-list"), $(".broadOtherAmout").hide(), changeCharge(), 
    removePointPay()) : "10" == e && ($("#broadBandfix").hide(), $("#fixNumInput").hide(), 
    $(".septal-line").hide(), $(".broad").addClass("broadCardList"), $(".otherBrandInput").val(""), 
    "02" == a && $(".broadbtn").show(), $(".broadOtherAmout").hide(), $("#broadNumber").show(), 
    $("#other").addClass("amount-list"), changeCharge(), removePointPay()), $("#broadHisNumDiv").length > 0 && ("03" == common.netCode ? ($("#fixNumInput").unbind("blur"), 
    $("#fixNumInput").bind("blur", function() {
        $("#broadHisNumDiv").hide();
    }), $("#broadHisNumDiv").mouseover(function() {
        $("#fixNumInput").unbind("blur");
    }), $("#broadHisNumDiv").mouseout(function() {
        $("#fixNumInput").bind("blur", function() {
            $("#broadHisNumDiv").hide();
        });
    })) : "04" == common.netCode ? ($("#broadBandAccount").unbind("blur"), $("#broadBandAccount").bind("blur", function() {
        $("#broadHisNumDiv").hide(), broadBlur();
    }), $("#broadHisNumDiv").mouseover(function() {
        $("#broadBandAccount").unbind("blur");
    }), $("#broadHisNumDiv").mouseout(function() {
        $("#broadBandAccount").bind("blur", function() {
            $("#broadHisNumDiv").hide();
        });
    })) : "08" == common.netCode && ($("#broadBandPhone").unbind("blur"), $("#broadBandPhone").bind("blur", function() {
        $("#broadHisNumDiv").hide(), broadBlur();
    }), $("#broadHisNumDiv").mouseover(function() {
        $("#broadBandPhone").unbind("blur");
    }), $("#broadHisNumDiv").mouseout(function() {
        $("#broadBandPhone").bind("blur", function() {
            $("#broadHisNumDiv").hide();
        });
    }))), 1 == isPackYear) {
        $(".payfeeTypeHref").hide();
        for (var r = !1, o = 0; o < broadband.broadNumberType.length; o++) ("4" == broadband.broadNumberType[o] || "10" == broadband.broadNumberType[o]) && (r = !0);
        "10" != e && "04" != e && "08" != e || 1 != r || $("#idEnter").show();
    }
    "04" == e && "" == $(".otherBrandInput").val() && ($(".amount-list-broad").removeClass("amount-cur"), 
    $(".payFeeMoney b").html("0.00元"), $(".total b").html("0.00")), computedHei();
};

