var isBroadBalanceOnline = function(e, d) {
    $.ajax({
        type: "post",
        data: {
            "commonBean.provinceCode": e,
            "commonBean.phoneNo": d
        },
        dataType: "json",
        url: serviceBankchargeURL + "broadbandPackYear/isBroadBalaceProvinceOnline"
    }).success(function(e) {
        1 == e.isOnline ? $(".payfeeTypeHref").show() : $(".payfeeTypeHref").hide();
    }).error(function() {
        $(".payfeeTypeHref").hide();
    });
