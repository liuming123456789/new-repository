}, makeActivityStr = function(e, a) {
    if ("1" == e.activityPayFeeExsits) {
        cards.activityAmountLimit = e.activityAmountLimit, cards.activityPayFeeExsits = e.activityPayFeeExsits, 
        cards.payfeeActivity_type = e.payfeeActivity_type, cards.gift_amount = e.gift_amount, 
