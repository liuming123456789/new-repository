    var r = "";
    r = "08" == e ? a / 100 : 1e3 * a / 100;
    var t = offerDesc[e];
    return judgeEmpty(t) ? r + offerDesc.normal : r + t;
}, calOfferMoney = function(e, a) {
    return "省" + (Number(e) / 100 - Number(a)).toFixed(2) + "元：";
}, calOfferPriceStr = function(e, a, r) {
    if ("08" == e) {
