            $(".newProduct").html(t), $(".payFeeTip").show();
        }
        charge.isActionPayfee = !0, $(".business").find(".detail i").removeClass("checked"), 
        $(this).addClass("checked"), $(".business").find(".detail").css("backgroundColor", "#fff"), 
        $(this).parent().css("backgroundColor", "#ffece5"), mustpayFeeInfo > 0 && common.phoneRex.test(formateNumGetBasicPhone($("#number").val())) && judgeEmpty(isMobInputMoney) ? ($(".otherInput").eq(0).val(mustpayFeeInfo + "元").addClass("num-ipt05 num-ipt06").removeClass("num-ipt04").css("color", "#999"), 
