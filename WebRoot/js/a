}, clearAccount = function() {
    $(".custominfos,.customMustPay").text(""), $(".User_information").hide(), $(".broadbtn button").addClass("gray"), 
    $(".unQuery").hide(), $(".chosen").hide(), $(".hasChosen").hide(), $("#broadNumber").val("").addClass("broad-nobold").removeClass(".broad-bold"), 
    $("#broadBandAccount").val("").addClass("broad-nobold").removeClass(".broad-bold"), 
