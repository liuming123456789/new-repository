                            var h = Math.abs(t) - 90 * Math.floor(Math.abs(t) / 90), g = 1.5 - (Math.sin(2 * h * Math.PI / 360) / 2 + Math.cos(2 * h * Math.PI / 360) / 2), f = b.params.cube.shadowScale, v = b.params.cube.shadowScale / g, w = b.params.cube.shadowOffset;
