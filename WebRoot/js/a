    n.match(/(Android);?[\s\/]+([\d.]+)?/) ? !0 : !1), t = n.match(/(iPad).*OS\s([\d_]+)/) ? !0 : !1, i = !t && n.match(/(iPhone\sOS)\s([\d_]+)/) ? !0 : !1, a = (i || t) && n.match(/Safari/), r = (n.match(/MQQBrowser\/([\d\.]+)/) ? !0 : !1, 
    0);
    a && (r = n.match(/Version\/([\d\.]+)/)), o ? js_invoke.interact('{"type":"login" ,"url":"' + e + '"}') : window.location = 'clientAction={"type":"login" ,"url":"' + e + '"}';
