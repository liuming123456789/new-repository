        r += '<li class="cashVoucher-list" onclick="selectCashVoucher.call(this);" offerRateFlag="' + t.offerRateFlag + '" offerRateId="' + t.offerRateInfo[0] + '" offerRate="' + t.offerRateInfo[1] + '" ticketNo="' + o + '"> <div class="leftCircle"></div> <div class="allCash"> <div class="cashVoucher-left"> ' + c + '</div><div class="cashVoucher-right"> <h3>固定面值充值</h3> <p>' + t.allowCardVal + "元面值可用</p> <span>" + s + "-" + i + '</span> </div> <div class="usebtn' + (d ? " used" : "") + '">' + (d ? "已选" : "使用") + '</div> </div> <div class="rightCircle"></div> </li>';
    }), e.length > 1 && (r = t + r), r;
}, selectCashVoucher = function() {
    ModalHelper.beforeClose();
    var e = $(this), a = e.attr("offerRate"), r = e.attr("offerRateFlag"), t = e.attr("offerRateId"), s = e.attr("ticketNo");
    cards.selectedOfferRateId = t, cards.chooseTicketNo = "", ("07" == r || "08" == r) && (cards.chooseTicketNo = s);
    var i = isMobileTab() ? cards.cardinfoList : isFixedTab() ? cards.cardinfoFixList : cards.cardinfoBroadList, o = createCardList(i), c = $(isMobileTab() ? ".mobile-cardlist" : isFixedTab() ? ".fixed-cardlist" : ".broad-cardlist");
    c.html(o);
    var d = getOfferRateName(r, a);
    $(".inner-cashVoucher .cur-cashVoucher").html(d), $("#cashVoucherPage .btmPart").animate({
        bottom: "-100%"
    }, "fast", function() {
        $("#cashVoucherPage").hide();
    }), $("#cashVoucherPage .usebtn").text("使用").removeClass("used"), $(this).find(".usebtn").text("已选").addClass("used");
};function getEvent() {
    if (window.event) return window.event;
    for (func = getEvent.caller; null != func; ) {
        var e = func.arguments[0];
        if (e && (e.constructor == Event || e.constructor == MouseEvent || e.constructor == KeyboardEvent || "object" == typeof e && e.preventDefault && e.stopPropagation)) return e;
        func = func.caller;
    }
    return null;
}

function cancelBubble() {
    var e = getEvent();
    window.event ? e.cancelBubble = !0 : e.preventDefault && e.stopPropagation();
}

function getCBRealActivityPayFeeContent() {
    var e = charge.provinceCode, t = charge.cityCode, a = $("#number").val().replace(/\s/g, "");
    checkUrl = ISWAP ? "mobWapActionpayfee/getActionPayFeeList.action" : "mobAppActionpayfee/getActionPayFeeList.action";
    var o = "https://upay.10010.com/npfwap/NpfMob/" + checkUrl;
    $.ajax({
        url: o,
        type: "get",
        data: "commonBean.provinceCode=" + e + "&commonBean.cityCode=" + t + "&commonBean.phoneNo=" + a,
        jsonp: "callback",
        jsonpCallback: "getActivities",
        dataType: "jsonp",
        success: function(e) {
            if (judgeEmpty(e)) return charge.isActionPayfee = !1, $(".action-list").addClass("gray-amount-list"), 
            void $(".actions").hide();
            var t, a = e, o = a.actionList, n = a.isFtp;
            if (n || (t = a.productinfo[0].productid), 0 == o.length) return charge.isActionPayfee = !1, 
            $(".action-list").addClass("gray-amount-list"), void $(".actions").hide();
            charge.isActionPayfee = !0, $(".action-list").removeClass("gray-amount-list"), $(".actions").show();
            for (var i = "", r = 0; r < o.length; r++) (n || "" == o[r].productCode || -1 != o[r].productCode.indexOf(t)) && (i += '<div class="swiper-slide">' + o[r].actionName + "</div>");
            if (judgeEmpty(i)) for (var r = 0; r < o.length; r++) i += '<div class="swiper-slide">' + o[r].actionName + "</div>";
            $(".arrow-top").html(i);
            var c = (new Swiper("#actionSwiper", {
                autoplay: 3e3,
                autoplayDisableOnInteraction: !1,
                observer: !0,
                loop: !0
            }), GetRequest("serviceType"));
            "29" == c && clickActionPayfee();
        },
        error: function() {
            charge.isActionPayfee = !1, $(".action-list").addClass("gray-amount-list");
        }
    });
}

function jumpNext() {
    var e = charge.provinceCode, t = charge.cityCode, a = $("#number").val().replace(/\s/g, ""), o = !1;
    (common.isLogin && common.loginPhoneNo == a || isFtp) && (o = !0);
    var n = "../actionpayfee/index.html?phoneNo=" + a + "&provinceCode=" + e + "&cityCode=" + t + "&isIdentical=" + o;
    window.open(n, "_self");
}

function actionPayFeeSubmit() {
    charge.isFirstPaySubmit = !1;
    var e = !0;
    if ("none" == $(".balance").css("display") && (e = !1, $(".agreement i").css("marginLeft", ".42rem")), 
    $(".balance").find("i").hasClass("bal-checked") || (e = !1), !$(".agreement").find("i").hasClass("bal-checked")) return isAgreeAgreement = !1, 
    $("#errormsg").html("请勾选存费送费协议").show(), errormsgshow(), !1;
    isAgreeAgreement = !0, checkurl = ISWAP ? "mobWapActionpayfee/" : "mobAppActionpayfee/";
    var t;
    t = e ? serviceActionPayfeeUrl + checkurl + "actionPayFeeBalance.action" : serviceActionPayfeeUrl + checkurl + "actionPayFeeConfirm.action";
    var a = getCheckParam();
    $(".hiddenPhone").val(a["commonBean.phoneNo"]), $(".hiddenProvince").val(a["commonBean.provinceCode"]), 
    $.ajax({
        url: t,
        type: "post",
        async: !1,
        data: $("#bankchargeForm").serialize(),
        jsonp: "callback",
        jsonpCallback: "actionPayFee",
        dataType: "jsonp",
        success: function(e) {
            if (!judgeEmpty(e)) {
                var t = e, a = t.respCode, o = t.respDesc, n = t.respKey;
                if ("online" == n) {
                    if ("0000" != a) return $("#errormsg").html(o).show(), errormsgshow(), void (charge.isFirstPaySubmit = !0);
                    0 == parseInt(payAmount) ? ($("#paystate").val(t.secstate), $("#bankchargeForm").attr("action", serviceActionPayfeeUrl + checkurl + "actionPayFeeToSuccess.action"), 
                    $("#bankchargeForm").submit()) : window.open(t.redirectUrl, "_self");
                } else "0000" == a ? ($("#paystate").val(t.secstate), $("#bankchargeForm").attr("action", serviceActionPayfeeUrl + checkurl + "BalanceFormalSubmit.action"), 
                $("#bankchargeForm").submit()) : ($("#errormsg").html(o).show(), errormsgshow(), 
                charge.isFirstPaySubmit = !0);
            }
        },
        error: function() {
            $("#errormsg").html("提交异常 : 交费系统繁忙").show(), errormsgshow();
        }
    });
}

function showOneAction(e) {
    for (var t = "", a = 0; a < e.length; a++) {
        var o = e[a];
        o.actionConfigId == actionConfigId && (t += "<li class='detail'><i onclick='detailMsg.call(this)'></i><div onclick='choseIcon.call(this)'><p class='actionID' id='" + o.actionConfigId + "' price='" + o.actionMoney + "' feetype='" + o.actionfeeType + "'>" + o.actionName + "</p><p class='discount'><span>" + o.actionDesc + "</span><p class='cost'>￥" + o.actionMoney + "</p><b class='more' onclick='showMore.call(this)'>···</b></p><p class='detailDesc' style='display:none'>" + o.actionDetailDesc + "</p></div></li>");
    }
    return t;
}

function phoneInvoiceInfoActivityPayFee() {
    var e = phoneNo;
    judgeEmpty(e) || $.getJSON(nginxRequstURL + "getArea/init?callback=?", {
        "commonBean.phoneNo": formateNumGetBasicPhone(e)
    }, function(t) {
        if (charge.provinceCode = t.prov.split("X")[0], charge.cityCode = t.prov.split("X")[1], 
        void 0 == t.prov.split("X")[2]) return inputPhoneShow(!0), $("#errormsg").html("对不起,暂不支持该号码交费！").show(), 
        void errormsgshow();
        area.municipality.indexOf(charge.provinceCode) >= 0 ? "" : getProvinceName(charge.provinceCode), 
        getCityName(charge.provinceCode, charge.cityCode);
        getOneRealActivityPayFeeContent(e.replace(/\s+/g, "")), changeInputClass("number"), 
        inputPhoneShow(!0), $(".custominfos,.customMustPay").html(""), $("#errormsg").html("").hide();
        var a = document.getElementById("number"), o = getPosition(a);
        /^(\d|\s)$/.test($(a).val().charAt(o - 1)) || (o -= 1);
        var n = " " == $(a).val().charAt(o - 1);
        return $(a).val(formatePhone($(a).val())), " " != $(a).val().charAt(o - 1) || n || (o += 1), 
        setSelectionRange(a, o), !judgeEmpty($(a).val()) && formateNumGetBasicPhone($(a).val()).length > 11 ? ($("#errormsg").html("请输入正确的手机号码").show(), 
        void errormsgshow()) : (charge.getOfferate = !1, void $("#number").blur());
    });
}

