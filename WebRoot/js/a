        $(".mobileCardList span").removeClass("amount-cur"), $(".otherInput").eq(0).css("background", "#fff"), 
        $(".otherInput").eq(0).val(charge.otherAmount).addClass("num-ipt05").removeClass("num-ipt04 num-ipt06"), 
        $("label.left").eq(0).hide()), $(this).parent().siblings().css("backgroundColor", "#ffffff"), 
        $(".balance").show(), 1 == isShowAgreement ? ($(".agreement").show(), $(".agreement i").css("marginLeft", 0)) : $(".agreement").hide(), 
        $(".mobileCardList a").removeClass("amount-cur"), $("#payButton").removeClass("bg-gray"), 
        $(".btn-style-bottom").removeClass("bg-color"), $(".payFeeDetail").hide(), payAmount = $(this).parents(".detail").find(".actionID").attr("price"), 
        feetype = $(this).parents(".detail").find(".actionID").attr("feetype"), actionConfigId = $(this).parents(".detail").find(".actionID").attr("id"), 
