historyNum.isGetHis = !1, historyNum.isGetFixHis = !1, historyNum.isGetBroadBandHis = !1;

var canLoadhis = function() {
    return historyNum.historyNum.length > 0 || historyNum.isGetHis;
}, canFixLoadhis = function() {
    return historyNum.fixHistoryNum.length > 0 || historyNum.isGetFixHis;
}, canBroadBandLoadhis = function() {
    return historyNum.broadBandHistoryNum.length > 0 || historyNum.isGetBroadBandHis;
}, getHistoryNum = function(i) {
    if (0 == historyNum.historyNum.length && $("#historyNumDiv").hide(), !("mob" == i && canLoadhis() || "fix" == i && canFixLoadhis() || "broad" == i && canBroadBandLoadhis())) {
        var o = ISWAP ? "commUsedNum/WapGetCommUsedNumList" : "commUsedNum/GetCommUsedNumList";
        $.ajax({
            type: "POST",
            url: serviceQueryURL + o,
            data: {
                numType: i
            },
            dataType: "json"
        }).success(function(o) {
            "mob" == i ? (historyNum.isGetHis = !0, fillMobHistory(o)) : "fix" == i ? (historyNum.isGetFixHis = !0, 
            fillFixHistory(o)) : (historyNum.isGetBroadBandHis = !0, fillBroadBandHistory(o));
        }).error(function() {});
    }
}, clearHistoryNum = function(i) {
    if ($("#historyNumDiv").hide(), ("mob" != i || 0 != historyNum.historyNum.length) && !("fix" == i && 0 == historyNum.fixHistoryNum.length || "broad" == i && 0 == historyNum.broadBandHistoryNum.length)) {
        $("#broadHisNumDiv").find("ul").empty();
        var o = ISWAP ? "commUsedNum/WapClearCommUsedNumList" : "commUsedNum/ClearCommUsedNumList";
        $.ajax({
            type: "POST",
            url: serviceQueryURL + o,
            data: {
                numType: i
            },
            dataType: "json"
        }).success(function() {
            if ("mob" == i) historyNum.historyNum = [], $("#number").focus(), historyNum.isGetHis = !0, 
            fillMobHistory(""); else if ("fix" == i) historyNum.isGetFixHis = !0, historyNum.fixHistoryNum = [], 
            $("#fixPhone").focus(), fillFixHistory(""); else {
                historyNum.isGetBroadBandHis = !0, historyNum.broadBandHistoryNum = [];
                var o = "03" == common.netCode ? "fixNumInput" : "04" == common.netCode ? "broadBandAccount" : "broadBandPhone";
                $("#" + o).focus(), fillBroadBandHistory("");
            }
        }).error(function() {});
    }
}, fillBroadBandHistory = function(i) {
    for (var o = 0; o < i.length; o++) {
        historyNum.broadBandHistoryNum[o] = i[o], historyNum.broadBandHistoryNum[o].phone = i[o].phone, 
        historyNum.broadBandHistoryNum[o].customName = i[o].customName;
        var r = 2 == i[o].provinceCode.length ? "0" + i[o].provinceCode : i[o].provinceCode;
        historyNum.broadBandHistoryNum[o].PROVINCE_NAME = area.municipality.indexOf(i[o].provinceCode) >= 0 ? "" : getProvinceName(r), 
        historyNum.broadBandHistoryNum[o].CITY_NAME = getCityName(i[o].provinceCode, i[o].cityCode);
    }
    $("#broadHisNumDiv ul").html(createHis(historyNum.broadBandHistoryNum, "chooseBroadBandHistory", "broad"));
}, fillFixHistory = function(i) {
    for (var o = 0; o < i.length; o++) {
        historyNum.fixHistoryNum[o] = i[o], historyNum.fixHistoryNum[o].phone = i[o].phone, 
        historyNum.fixHistoryNum[o].customName = i[o].customName;
        var r = 2 == i[o].provinceCode.length ? "0" + i[o].provinceCode : i[o].provinceCode;
