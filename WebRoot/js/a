}, accMul = function(e, n) {
    var o = 0, t = e.toString(), i = n.toString();
    try {
        o += t.split(".")[1].length;
    } catch (a) {}
    try {
        o += i.split(".")[1].length;
    } catch (a) {}
    return Number(t.replace(".", "")) * Number(i.replace(".", "")) / Math.pow(10, o);
}, formatePhone = function(e) {
    if (null != e && void 0 != e && "" != e) {
        0 == e.indexOf("+86") && (e = e.substr(3));
        for (var n = "", o = 0, t = 0; t < e.length; t++) /^\d$/.test(e.charAt(t)) && 11 > o && (n += 3 == o || 7 == o || 11 == o ? " " + e.charAt(t) : e.charAt(t), 
        o++);
        return n;
    }
}, formateNumGetBasicPhone = function(e) {
    return null != e && void 0 != e && "" != e ? (0 == e.indexOf("+86") && (e = e.substr(3)), 
    e.length > 11 && 0 == e.indexOf("86") && (e = e.substr(2)), "16" == e.length && (e = e.substr(5)), 
    e.replace(/[^0-9]/gi, "").trim()) : void 0;
