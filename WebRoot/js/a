        var r = {};
        if (r.activityAmountLimit = e.activityAmountLimit, r.activitytype = e.payfeeActivity_type, 
        r.giftAmount = e.gift_amount, r.activityTips = e.activity_tips, r.trafficGetUrl = e.trafficGetUrl, 
        r.supportServiceType = e.supportServiceType, 0 == a.length && "7" != e.payfeeActivity_type) a.push(r); else {
            {
                var t = !0;
                new Array();
            }
            for (var s in a) if (a[s].activityAmountLimit == e.activityAmountLimit || "7" == e.payfeeActivity_type) {
                t = !1;
                break;
            }
            t && a.push(r);
        }
    }
    if (!judgeEmpty(e.activity_type)) {
        if (judgeEmpty(cards.activity_type) && (cards.activity_type = e.activity_type, cards.gift_amount = e.gift_amount, 
        cards.activity_tips = e.activity_tips, cards.trafficGetUrl = e.trafficGetUrl, cards.supportServiceType = e.supportServiceType), 
        1 != e.offerRate && 2 == e.activity_type) return "<b class='free_flow'></b>";
        if (1 != e.offerRate && 1 == e.activity_type) return "<b class='free_bill'></b>";
        if (1 != e.offerRate && 3 == e.activity_type) return "<b class='free_vip'></b>";
        if (1 != e.offerRate && 4 == e.activity_type) return "<b class='free_coupon'></b>";
        if (1 != e.offerRate && 0 == e.activity_type) return "<b class='free_random'></b>";
        if (1 != e.offerRate && 6 == e.activity_type) {
            if ("20190505" == e.rechargeId || "20190605" == e.rechargeId) return "<b class='sendMonthCard'></b>";
            if ("20190510" == e.rechargeId || "20190610" == e.rechargeId) return "<b class='sendQuarterCard'></b>";
            if ("20190530" == e.rechargeId || "20194100" == e.rechargeId || "20190630" == e.rechargeId) return "<b class='sendYearCard'></b>";
            if ("20190560" == e.rechargeId || "20194300" == e.rechargeId || "20190660" == e.rechargeId) return "<b class='sendThreeYearCard'></b>";
        } else {
            if (2 == e.activity_type) return "<b class='sendFlow'></b>";
            if (1 == e.activity_type) return "<b class='sendBill'></b>";
            if (3 == e.activity_type) return "<b class='sendVip'></b>";
            if (4 == e.activity_type) return "<b class='sendCoupon'></b>";
            if (0 == e.activity_type) return "<b class='sendRandom'></b>";
